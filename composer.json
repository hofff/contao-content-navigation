{
  "name": "hofff/contao-content-navigation",
  "description": "Provides a table of contents for elements based on one or more articles within the page.",
  "license": "LGPL-3.0-or-later",
  "type": "contao-bundle",
  "keywords": [
    "contao",
    "contents",
    "nabigation",
    "toc"
  ],
  "authors": [
    {
      "name": "Nicky Hoff",
      "email": "nick@hofff.com",
      "homepage": "https://www.hofff.com",
      "role": "Manager"
    },
    {
      "name": "David Molineus",
      "email": "david@hofff.com",
      "homepage": "https://www.hofff.com",
      "role": "Developer"
    }
  ],
  "homepage": "https://www.hofff.com",
  "require": {
    "php": "^7.2 || ^8.0",
    "ext-pdo": "*",
    "ausi/slug-generator": "^1.1",
    "contao/core-bundle": "^4.9",
    "doctrine/dbal": "^2.11 || ^3.0",
    "symfony/polyfill-mbstring": "^1.0",
    "symfony/config": "^4.4 || ^5.1",
    "symfony/dependency-injection": "^4.4 || ^5.1",
    "symfony/http-kernel": "^4.4 || ^5.1"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.1",
    "doctrine/coding-standard": "^9.0.",
    "phpcq/runner-bootstrap": "^1.0@dev"
  },
  "conflict": {
    "contao/manager-plugin": "<2.1 || >=3.0"
  },
  "autoload": {
    "psr-4": {
      "Hofff\\Contao\\ContentNavigation\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "contao-components/installer": false,
      "contao/manager-plugin": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    },
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1.x-dev"
    },
    "contao-manager-plugin": "Hofff\\Contao\\ContentNavigation\\ContaoManager\\Plugin"
  }
}
